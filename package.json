{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "start": "node dist/server.js",
    "deploy": "prisma migrate deploy && node dist/server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "railway:deploy": "prisma migrate deploy && node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run test/routes",
    "test:integration:watch": "vitest test/routes",
    "test:setup": "prisma migrate deploy",
    "test:db:reset": "prisma migrate reset --force"
  },
  "license": "MIT",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.0.1",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.11.1",
    "@scalar/fastify-api-reference": "^1.33.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.0",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^5.0.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.11.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^24.0.13",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
